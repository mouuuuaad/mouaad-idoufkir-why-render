{
  "name": "@mouaad_idoufkir/why-render",
  "version": "0.25.19",
  "type": "module",
  "description": "A comprehensive React 19 developer tool that detects unnecessary re-renders with visual debugging panels, performance monitoring, and AI-powered suggestions.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "import": "./dist/ui.js",
      "require": "./dist/ui.cjs"
    },
    "./overlays": {
      "types": "./dist/overlays.d.ts",
      "import": "./dist/overlays.js",
      "require": "./dist/overlays.cjs"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:ui": "tsup --entry.ui src/ui/index.ts --entry.overlays src/overlays/index.ts",
    "dev": "tsup --watch",
    "dev:demo": "cd demo && npm run dev",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "react-19",
    "performance",
    "re-render",
    "why-render",
    "optimization",
    "devtools",
    "debugging",
    "profiling",
    "flame-graph"
  ],
  "author": "Mouaad Idoufkir",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mouuuuaad/mouaad-idoufkir-why-render.git"
  },
  "homepage": "https://github.com/mouuuuaad/mouaad-idoufkir-why-render#readme",
  "bugs": {
    "url": "https://github.com/mouuuuaad/mouaad-idoufkir-why-render/issues"
  },
  "dependencies": {
    "zustand": "^4.5.0"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@types/d3-hierarchy": "^3.1.6",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.17",
    "d3-hierarchy": "^3.1.2",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "react": "^19.0.0-rc",
    "react-dom": "^19.0.0-rc",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vite": "^7.2.6",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}